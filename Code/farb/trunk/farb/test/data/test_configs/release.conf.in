#
# Releases
#
<Releases>
	# Release Build Working Directory
	BuildRoot @BUILDROOT@
	<Release 6.0>
		# FreeBSD CVS Repository Mirror
		CVSRoot	@CVSROOT@
		# Release Tag
		CVSTag	@TAG1@
	</Release>

	<Release 6-STABLE>
		# FreeBSD CVS Repository Mirror
		CVSRoot	@CVSROOT@
		# Release Tag
		CVSTag	@TAG2@
	</Release>
</Releases>

#
# Package Sets
#
<PackageSets>
	<PackageSet Base>
		<Package>
			Port            databases/mysql50-server
			Package         mysql50-server
			BuildOption     WITH_COLLATION=UTF8
		</Package>
	</PackageSet>
</PackageSets>

<Logging>
	<logfile>
		# Log debug messages to stdout
		Level debug
		Path STDOUT
	</logfile>
</Logging>
